<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>action</servlet-name>
        <init-param>
            <param-name>debug</param-name>
            <param-value>2</param-value>
        </init-param>
        <init-param>
            <param-name>detail</param-name>
            <param-value>2</param-value>
        </init-param>
        <load-on-startup>2</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>StartUpServlet</servlet-name>
        <servlet-class>Connectivity.ConfigServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>UploadReportServlet</servlet-name>
        <servlet-class>Reports.UploadReportServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Dashboard</servlet-name>
        <servlet-class>API.Dashboard</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LiveTracking</servlet-name>
        <servlet-class>API.LiveTracking</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserLogin</servlet-name>
        <servlet-class>master.UserLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Notifications</servlet-name>
        <servlet-class>API.Notifications</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReportGenerate</servlet-name>
        <servlet-class>API.ReportGenerate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Configurations</servlet-name>
        <servlet-class>API.Configurations</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserLogout</servlet-name>
        <servlet-class>master.UserLogout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FaultCodeDescription</servlet-name>
        <servlet-class>API.FaultCodeDescription</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CaptchaServlet</servlet-name>
        <servlet-class>master.CaptchaServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GenerateOTP</servlet-name>
        <servlet-class>API.GenerateOTP</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VerifyOTP</servlet-name>
        <servlet-class>API.VerifyOTP</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Activities</servlet-name>
        <servlet-class>API.Activities</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateDeviceToken</servlet-name>
        <servlet-class>API.UpdateDeviceToken</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FCReport</servlet-name>
        <servlet-class>API.FCReport</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DTCReport</servlet-name>
        <servlet-class>API.DTCReport</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RedStopLampReport</servlet-name>
        <servlet-class>API.RedStopLampReport</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReportsParams</servlet-name>
        <servlet-class>API.ReportsParams</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AvgValueFinder</servlet-name>
        <servlet-class>API.AvgValueFinder</servlet-class>
    </servlet>
    
    
    <servlet-mapping>
        <servlet-name>action</servlet-name>
        <url-pattern>*.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UploadReportServlet</servlet-name>
        <url-pattern>/UploadReportServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Dashboard</servlet-name>
        <url-pattern>/Dashboard</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LiveTracking</servlet-name>
        <url-pattern>/LiveTracking</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserLogin</servlet-name>
        <url-pattern>/UserLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Notifications</servlet-name>
        <url-pattern>/Notifications</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReportGenerate</servlet-name>
        <url-pattern>/Report</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Configurations</servlet-name>
        <url-pattern>/Configurations</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserLogout</servlet-name>
        <url-pattern>/UserLogout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FaultCodeDescription</servlet-name>
        <url-pattern>/FaultCodeDescription</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CaptchaServlet</servlet-name>
        <url-pattern>/captcha-image</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GenerateOTP</servlet-name>
        <url-pattern>/generateOTP</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VerifyOTP</servlet-name>
        <url-pattern>/verifyOTP</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Activities</servlet-name>
        <url-pattern>/activities</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateDeviceToken</servlet-name>
        <url-pattern>/updateDeviceToken</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FCReport</servlet-name>
        <url-pattern>/FCReport</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DTCReport</servlet-name>
        <url-pattern>/DTCReport</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RedStopLampReport</servlet-name>
        <url-pattern>/RedStopLampReport</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReportsParams</servlet-name>
        <url-pattern>/ReportsParams</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AvgValueFinder</servlet-name>
        <url-pattern>/AvgValueFinder</url-pattern>
    </servlet-mapping>
    
    

    <session-config>
        <session-timeout>
            3
        </session-timeout>
        <cookie-config>
            <secure>true</secure>
            <!--            <http-only>true</http-only>-->
        </cookie-config>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>
